{% extends 'SonataAdminBundle:CRUD:base_list_field.html.twig' %}

{% block field %}
    {% if not object.isPublished %}
        <span class="label label-warning">Dépubliée</span>
    {% elseif object.isCancelled %}
        <span class="label label-danger">Annulée</span>
    {% else %}
        <span class="label label-primary">Programmée</span>
    {% endif %}
{% endblock %}
